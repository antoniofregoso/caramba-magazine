<odoo>
  <data>
  	<record id="res_config_settings_view_form" model="ir.ui.view">
    	<field name="name">res.config.settings.view.form.inherit.search</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="20"/>
        <field name="inherit_id" ref="website.res_config_settings_view_form"/>
        <field name="arch" type="xml">
        <xpath expr="//div[@id='cdn_setting']" position="after">
        	<div id="website_search" class="col-12 col-md-6 o_setting_box">
            	<div class="o_setting_left_pane">
                	
                </div>
                <div class="o_setting_right_pane">
                	<label string="Search" for="search_blog_id"/> 
            		 		 <div class="text-muted">
                                 Blog to Search
                            </div>
                            <div class="mt8">
                            	<field name="search_blog_id" widget="selection"/>
                            </div>
               </div>
             </div>
            </xpath>
        </field>
	</record>            
 
  </data>
</odoo>